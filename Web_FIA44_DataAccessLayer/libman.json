{
  "version": "1.0",
  "defaultProvider": "jsdelivr",
    "libraries": [
        {
            "library": "bootstrap@5.3.3",
            "destination": "wwwroot/css/bootstrap/"
        },
        {
            "library": "jquery@3.7.1",
            "destination": "wwwroot/js/jquery/"
        },
        {
            "provider": "jsdelivr",
            "library": "@abp/jquery-validation@8.3.1",
            "destination": "wwwroot/js/jquery/jquery-validation/"
        },
        {
            "provider": "jsdelivr",
            "library": "@abp/jquery-validation-unobtrusive@8.3.1",
            "destination": "wwwroot/js/jquery/jquery-validation-unobtrusive/"
        }
    ]
}